<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
       http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.0.xsd"
       default-autowire="byName">
	<!--[hsf uic consumer bean] -->
	<bean id="clientInfo" class="com.taobao.uic.common.util.ClientInfo">
		<property name="appName" value="yourproject"></property>
		<property name="email" value="youremail@taobao.com"></property>
	</bean>
	<bean id="uicCacheService" class="com.taobao.uic.common.cache.DefaultUICCacheService" init-method="init">
		<property name="configServerVersion" value="${yourproject.consumer.uic.configserver.version}" />
		<property name="securityService" ref="securityService" />
	</bean>
	<bean id="securityService" class="com.taobao.uic.security.service.impl.SecurityClient" >
    	<property name="configServerVersion" value="${yourproject.consumer.uic.service.version}" />
    </bean> 
    <bean id="uicPaymentAccountService" class="com.taobao.hsf.app.spring.util.HSFSpringConsumerBean" init-method="init">
		<property name="interfaceName" value="com.taobao.uic.common.service.userinfo.UicPaymentAccountService" />
		<property name="version" value="${yourproject.consumer.uic.service.version}" />
	</bean>
	<bean id="uicPaymentAccountServiceClient" class="com.taobao.uic.common.service.userinfo.client.UicPaymentAccountServiceClient">
		<property name="clientInfo" ref="clientInfo" />
		<property name="uicPaymentAccountService" ref="uicPaymentAccountService" />
		<property name="uicCacheService" ref="uicCacheService" />
	</bean>
	<bean id="uicExtraReadService" class="com.taobao.hsf.app.spring.util.HSFSpringConsumerBean" init-method="init">
		<property name="interfaceName" value="com.taobao.uic.common.service.userinfo.UicExtraReadService" />
		<property name="version" value="${yourproject.consumer.uic.service.version}" />
	</bean>
	<bean id="uicExtraReadServiceClient" class="com.taobao.uic.common.service.userinfo.client.UicExtraReadServiceClient">
		<property name="clientInfo" ref="clientInfo" />
		<property name="uicExtraReadService" ref="uicExtraReadService" />
		<property name="uicCacheService" ref="uicCacheService" />
	</bean>
	<bean id="uicReadService" class="com.taobao.hsf.app.spring.util.HSFSpringConsumerBean" init-method="init">
		<property name="interfaceName" value="com.taobao.uic.common.service.userinfo.UicReadService" />
		<property name="version" value="${yourproject.consumer.uic.service.version}" />
	</bean>
	<bean id="uicReadServiceClient" class="com.taobao.uic.common.service.userinfo.client.UicReadServiceClient">
		<property name="clientInfo" ref="clientInfo" />
		<property name="uicReadService" ref="uicReadService" />
		<property name="uicExtraReadServiceClient" ref="uicExtraReadServiceClient" />
		<property name="uicCacheService" ref="uicCacheService" />
		<property name="securityService" ref="securityService"/>
	</bean>
	<bean id="uicDataService" class="com.taobao.hsf.app.spring.util.HSFSpringConsumerBean" init-method="init">
		<property name="interfaceName" value="com.taobao.uic.common.service.userdata.UicDataService" />
		<property name="version" value="${yourproject.consumer.uic.service.version}" />
	</bean>
	<bean id="uicDataServiceClient" class="com.taobao.uic.common.service.userdata.client.UicDataServiceClient">
		<property name="clientInfo" ref="clientInfo" />
		<property name="uicDataService" ref="uicDataService" />
		<property name="uicCacheService" ref="uicCacheService" />
	</bean>
	<!-- hsf message consumer bean -->
	<bean id="messageSenderService" class="com.taobao.hsf.app.spring.util.HSFSpringConsumerBean" init-method="init">
		<property name="interfaceName" value="com.taobao.messenger.service.MessageSenderService" />
		<property name="version" value="${yourproject.consumer.messenger.service.version}" />
	</bean>

</beans>
