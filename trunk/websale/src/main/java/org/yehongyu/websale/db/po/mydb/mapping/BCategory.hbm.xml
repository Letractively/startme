<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<!-- 
        b2b hibernate hbm.xml 
        
-->
    <class name="org.yehongyu.websale.db.po.mydb.BCategory" table="B_CATEGORY" >
	<!--PK-->
        <id name="id" column="CATID" type="string" >
            <generator class="assigned"/>
        </id>

        <property name="catname" type="string">
             <column name="CATNAME" />
        </property>
        <property name="pcatid" type="string">
             <column name="PCATID" />
        </property>
        <property name="sortnum" type="long">
             <column name="SORTNUM" />
        </property>
        <property name="state" type="long">
             <column name="STATE" />
        </property>
    </class>
</hibernate-mapping>