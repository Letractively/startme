<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<!-- 
        b2b hibernate hbm.xml 
        
-->
    <class name="org.yehongyu.websale.db.po.mydb.SysUserMenu" table="SYS_USER_MENU" >
	<!--PK-->
        <id name="id" column="PMID" type="long" >
            <generator class="sequence">
                <param name="sequence">SYS_USER_MENU_SEQ</param>
            </generator>
        </id>

        <property name="userid" type="long">
             <column name="USERID" />
        </property>
        <property name="menuid" type="long">
             <column name="MENUID" />
        </property>
    </class>
</hibernate-mapping>