<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<!-- 
        b2b hibernate hbm.xml 
        
-->
    <class name="org.yehongyu.websale.db.po.mydb.BProduct" table="B_PRODUCT" >
	<!--PK-->
        <id name="id" column="PRODUCTID" type="long" >
        	<generator class="increment"/>
<!--            <generator class="sequence">-->
<!--                <param name="sequence">B_PRODUCT_SEQ</param>-->
<!--            </generator>-->
        </id>

        <property name="price" type="big_decimal">
             <column name="PRICE" />
        </property>
        <property name="num" type="long">
             <column name="NUM" />
        </property>
        <property name="introduce" type="string">
             <column name="INTRODUCE" />
        </property>
        <property name="picname" type="string">
             <column name="PICNAME" />
        </property>
        <property name="pubdate" type="timestamp">
             <column name="PUBDATE" />
        </property>
        <property name="pubman" type="string">
             <column name="PUBMAN" />
        </property>
        <property name="pubtimes" type="long">
             <column name="PUBTIMES" />
        </property>
        <property name="state" type="long">
             <column name="STATE" />
        </property>
        <property name="catid" type="string">
             <column name="CATID" />
        </property>
        <property name="bookname" type="string">
             <column name="BOOKNAME" />
        </property>
        <property name="bookwriter" type="string">
             <column name="BOOKWRITER" />
        </property>
        <property name="bookpublish" type="string">
             <column name="BOOKPUBLISH" />
        </property>
        <property name="bookpubdate" type="timestamp">
             <column name="BOOKPUBDATE" />
        </property>
        <property name="saveindate" type="timestamp">
             <column name="SAVEINDATE" />
        </property>
        <property name="ispopulate" type="long">
             <column name="ISPOPULATE" />
        </property>
        <property name="iscommend" type="long">
             <column name="ISCOMMEND" />
        </property>
    </class>
</hibernate-mapping>